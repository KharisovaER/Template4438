<Window x:Class="Template4438._4438_Kharisova"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Template4438"
        mc:Ignorable="d"
        Title="_4438_Kharisova" Height="450" Width="800">
    <Window.Resources>
        <Style x:Key="BtnStyle">
            <Setter Property="Control.Width" Value="300"></Setter>
            <Setter Property="Control.Height" Value="50"></Setter>
            <Setter Property="Control.FontSize" Value="20"></Setter>
        </Style>
    </Window.Resources>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <TextBox Grid.Row="0"  Text="Elvira, 18 y.o." TextAlignment="Center" FontSize="50"></TextBox>
        <Button Grid.Row="1"  x:Name="BtnImport" Click="BtnImport_Click" Style="{StaticResource BtnStyle}">
            Import data
        </Button>
        <Button Grid.Row="2"  x:Name="BtnExport" Click="BtnExport_Click" Style="{StaticResource BtnStyle}">
            Export data
        </Button>
    </Grid>
</Window>
